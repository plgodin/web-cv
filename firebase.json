{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ ],
    "rewrites": [
      {
        "source": "/cv",
        "destination": "/cv.html"
      },
      {
        "source": "/noel",
        "destination": "/noel.html"
      },
      {
        "source": "!/@(js|css)/**",
        "destination": "/index.html"
      }
    ]
  }
}

{
  "hosting": {
    "public": "public",
    "predeploy": [
      "cd logprob && npm install && npm run build"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/cv",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/cv",
        "destination": "/cv.html"
      },
      {
        "source": "/noel",
        "destination": "/noel.html"
      },
      {
        "source": "/logprob/**",
        "destination": "/logprob/index.html"
      }
    ]
  }
}
